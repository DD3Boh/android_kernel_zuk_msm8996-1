&red_led {
	linux,name = "led:rgb_red";
	qcom,start-idx = <0x0>;
	qcom,duty-pcts = [00 12 24 36 48 63 63 63 48 36 24 12 00];
			qcom,duty-pcts1 = [00 16];
	qcom,lut-flags = <0x11>;
	qcom,pause-lo = <0xbb8>;
	/delete-property/ linux,default-trigger;
};
		
