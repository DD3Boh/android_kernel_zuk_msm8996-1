&pmi8994_flash0 {
	qcom,current = <800>;
};

&pmi8994_flash1 {
	qcom,current = <400>;
};

&pmi8994_torch0 {
	qcom,current = <200>;
};

&pmi8994_torch1 {
	qcom,current = <200>;
};
